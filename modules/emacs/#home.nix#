
 inputs, config, pkgs, lib, ... }:

with ../../functions;

let
  myEmacs = (pkgs.emacsWithPackagesFromUsePackage {
    config = ./inits/init.el;
    defaultInitFile = true;
    alwaysEnsure = true;
    package = pkgs.emacsGit;
    # override = epkgs: epkgs // {};
  });
in rec 
{
  programs.emacs = {
    enable = true;
    package = myEmacs;
  };
  home.file.".emacs.d/themes/catppuccin-theme.el".source = ./inits/catppuccin.el;
}

